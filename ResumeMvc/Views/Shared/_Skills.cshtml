@model IEnumerable<ResumeMvc.Models.Skill>


    <section id="skills">
        <h2 class="underlined">Skills</h2>
        <div class="skill">
            <div class="name">
                <strong> Core Competencies </strong>
            </div>
            <ul class="keywords">
                <li>.NET/Visual Studio</li>
                <li>Xamarin Android</li>
                <li>Windows Azure</li>
                <li>Windows Services</li>
                <li>Debugging</li>
                <li>Software Design</li>
            </ul>
        </div>
        <div class="skill">
            <div class="name">
                <strong> Android Development </strong>
            </div>
            <ul class="keywords">
                <li>Xamarin</li>
                <li>MvvmCross</li>
                <li>Xamarin Forms</li>
            </ul>
        </div>
        <div class="skill">
            <div class="name">
                <strong> Languages </strong>
            </div>
            <ul class="keywords">
                <li>C#</li>
                <li>SQL</li>
                <li>JSON</li>
                <li>XML</li>
                <li>XSLT</li>
                <li>Perl</li>
            </ul>
        </div>
        <div class="skill">
            <div class="name">
                <strong> Development Tools </strong>
            </div>
            <ul class="keywords">
                <li>Visual Studio 2003 - 2013</li>
                <li>SQL Management Studio 2008</li>
                <li>Moq</li>
                <li>Ninject</li>
                <li>NUnit</li>
                <li>Perforce</li>
                <li>Git</li>
                <li>Vim</li>
            </ul>
        </div>
        <div class="skill">
            <div class="name">
                <strong> Technologies </strong>
            </div>
            <ul class="keywords">
                <li>.NET 4.0</li>
                <li>Web Services (WCF)</li>
                <li>WPF</li>
                <li>Windows Services</li>
                <li>Regular Expressions</li>
                <li>MS Message Queues</li>
                <li>WMI</li>
            </ul>
        </div>
    </section>

    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <script>
        var highestBox = 0;
        $('.skill').each(function () {

            if ($(this).height() > highestBox)
                highestBox = $(this).height();
        })

        $('.skill').each(function () {
            $(this).height(highestBox);
        })
    </script>